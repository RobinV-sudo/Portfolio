 <body class="background">
  <div class="navbar">
    <%= link_to root_path do %>
      <%= image_tag ('/assets/logo_rv.png'), alt: ("logo rv"), class: ("logo_style") %>
    <% end %>
    <a class="navbar-item" href="/contact">Contact</a>
    <a class="navbar-item" href="/about">About</a>
    <a class="navbar-item" href="/project">Project</a>
    <a class="navbar-item" href="/">Home</a>
  </div>
  <div class="row">
    <div class="col-md-6">
      <div class="slidshow_container">
        <div class="fade mySlides">
        <%= image_tag ('/assets/about_rv.jpg'), alt: ('Photo Robin Vorstedt about'), class: ('photo_about_rv') %>
        </div>
        <div class="fade mySlides">
          <%= image_tag ('/assets/taipai_rv.jpg'), alt: ('Photo Robin Vorstedt about'), class: ('photo_about_rv') %>
        </div>
        <div class="fade mySlides">
          <%= image_tag ('/assets/beach_rv.jpg'), alt: ('Photo Robin Vorstedt about'), class: ('photo_about_rv') %>
        </div>
      </div>
      <% javascript_include_tag('slideshow') %>
        <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a>
    </div>
    <div class="col-md-6">
      <div class="about_main_card">
        <h2 class="about_main_card_title">About Robin Vorstedt</h2>
        <p class="about_main_card_text"></p>
      </div>
    </div>
  </div>
</body>
