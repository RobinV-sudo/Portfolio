<!DOCTYPE html>
<html>
  <head>
    <title>Portfolio</title>
    <%= favicon_link_tag %>
      <link rel="icon" type="image/x-icon" href="/favicon.ico">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= stylesheet_link_tag "_background", media: "all", "data-turbo-track": "reload" %>
      <%= stylesheet_link_tag "container", media: "all", "data-turbo-track": "reload" %>

    <%= yield %>

  </body>
</html>
